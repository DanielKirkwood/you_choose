rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    match /restaurants/{name} {
      allow read, write: if
          request.time < timestamp.date(2023, 6, 17);
    }
    match /groups/{name} {
          allow read, write: if
              request.time < timestamp.date(2023, 6, 17);
        }
    match /users/{name} {
              allow read, write: if
                  request.time < timestamp.date(2023, 6, 17);
            }
  }
}